// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id String @id @default(cuid())
  username String
  password String
}

model channel{
  id  Int  @id @default(autoincrement())
  channelName String
  link String
  artwork String
}

model subscriptions{
  id String @id @default(cuid())
  userid String
  channelid Int
}